{
    "name": "ghasedak/laravel-notification",
    "type": "library",
    "version": "1.0.0",
    "description": "Ghasedak SMS Notifications Channel for Laravel",
    "homepage": "https://github.com/ghasedakapi/Ghasedak-Laravel-Notification",
    "license": "MIT",
    "authors": [
        {
            "name": "Elham Salimi",
            "email": "salimiElham65@gmail.com",
            "role": "Developer"
        },{
            "name": " Ghasedak Team",
            "email": "qazvin@gmail.com",
            "role": "Company"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ghasedakapi/Ghasedak-Laravel-Notification"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ghasedak/laravel": "~1.0.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Ghasedak\\LaravelNotification\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ghasedak\\LaravelNotification\\GhasedakNotificationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
